    "devDependencies": {
        "babel-cli": "6.16.0",
        "babel-core": "6.17.0",
        "babel-loader": "6.2.5",
        "babel-preset-latest": "6.16.0",
        "babel-register": "6.16.3",
        "chai": "3.5.0",
        "eslint": "3.8.1",
        "eslint-plugin-import": "2.0.1",
